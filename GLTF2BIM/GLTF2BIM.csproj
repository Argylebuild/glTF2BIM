<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net47</TargetFramework>
    <AssemblyName>Argyle.glTF2BIM</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\StringLib.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>StringLib.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\StringLib.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>StringLib.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <Content Include="bin\Debug\net47\Argyle.glTF2BIM.dll" />
    <Content Include="bin\Debug\net47\Argyle.glTF2BIM.pdb" />
    <Content Include="bin\Debug\net47\Newtonsoft.Json.dll" />
  </ItemGroup>

</Project>
